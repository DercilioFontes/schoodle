<!DOCTYPE html>
<html lang="en">
<head>
  <title>Schoodle</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">



  <!-- Bootstrap CDN -->
  <!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous"> -->
  <link rel="stylesheet" type="text/css" href="/styles/main.css">
  <!-- <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> -->
  <!-- <link rel="stylesheet" href="styles/layout.css"> -->
  <!-- <link rel="stylesheet" href="styles/nav.css"> -->

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Nunito:300|Raleway" rel="stylesheet">

  <!-- Date and Time Picker CSS -->
  <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.8.2/fullcalendar.min.css' />
  <link rel='stylesheet' href="https://cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.css">

</head>

<body>
  <header>
    <h1><%= title %></h1>
    <h2><%= organizer_name %></h2>
    <h3><%= organizer_email %><h3></h3>
  </header>

  <!-- Open container -->
  <div class="container">
    <table>    
    <% Object.keys(slots).forEach( function (key) { %> 
      <thead>
        <tr>
          <th><%= moment(slots[key].date).format('YYYY-MM-DD') %> (<%= slots[key].start_time %> - <%= slots[key].end_time %>)</th>
        </tr>
      </thead>
    <% }) %>
      <tbody>
          
      </tbody>
  </table>
</div> 
<!-- Close container -->


</body>




