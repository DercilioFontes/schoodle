<!DOCTYPE html>
<html lang="en">
<head>

 <title>Schoodle</title>
 <meta charset="UTF-8">
 <meta name="viewport" content="width=device-width, initial-scale=1">

 <link rel="stylesheet" type="text/css" href="/styles/main.css">

 <!-- Google Fonts -->
 <link href="https://fonts.googleapis.com/css?family=Nunito:300|Raleway" rel="stylesheet">

 <!-- Date and Time Picker CSS -->
 <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.8.2/fullcalendar.min.css' />
 <link rel='stylesheet' href="https://cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.css">

</head>

<body>

 <header>
   <h1><%= title %></h1>
   <h2><%= organizerName %></h2>
   <h3><%= organizerEmail %><h3></h3>
 </header>

 <h1 id="countSlotIDs" hidden><%- JSON.stringify(countSlotIDs) %></h1>
 <h1 id="slotsData" hidden><%- JSON.stringify(slots) %></h1>

<table id= "slots">
  <tr>
    <th><h3>Name</h3></th>
    <% for (let slot of slots) { %>
      <th id="<%= slot.id %>"> <%= slot.date %><br><%= slot.start_time + "/" + slot.end_time %></br> Count: <%= countSlotIDs[slot.id] %> </th>
    <%}%>
  </tr>

</table>

<canvas id="barChart"></canvas>

<button id="attendee-done">Done</button>

<script type=“text/javascript” src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.bundle.js"></script>
<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript" src="/scripts/events.js"></script>
<script type="text/javascript" src="/scripts/result-char-bar.js"></script>


</body>
