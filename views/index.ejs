<!DOCTYPE html>
<html lang="en">
<head>
  <title>Schoodle</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- External CSS-->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link rel="stylesheet" href="styles/layout.css">
  <link rel="stylesheet" href="styles/main.css">
  <link rel="stylesheet" href="styles/nav.css">

  <link rel='stylesheet' href='https://fullcalendar.io/js/fullcalendar-3.1.0/fullcalendar.min.css' />

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
  <!-- <style>
  td.down {
    background-color: red;
  }

  </style> -->

</head>
<body>
  <!-- top nav bar -->
  <nav id="nav-bar">
    <img class="logo" src="http://www.fillmurray.com/200/200">
    <span class="header">Schoodle!</span>
  </nav>
<main class="container">
    <h2 class="page-head">Create your event!</h2>
  <div id="event">
    <form method="POST" action="/login" id="eventForm">
      <div class="eventForm">
        <i class="fa fa-user" style="font-size:36px"></i>
        <input text="text" name="userName" id="userName" placeholder="Enter name">
        <i class="material-icons" style="font-size:36px">email</i>
        <input text="text" name="email" id="email" placeholder="Enter email" >
        <i class="material-icons" style="font-size:36px">title</i>
        <input text="text" name="title" id="title" placeholder="Enter title">
        <i class="material-icons" style="font-size:36px">room</i>
        <input text="text" name="location" id="location" placeholder="Add location">
        <i class="fa fa-sort-alpha-desc" style="font-size:36px"></i>
        <input type="text" name="description" id="description" placeholder="Add note">
      </div>
      <button class="create-event">Create</button>
  </div>
</main>

  <div>
     <input text="text" id="selecttime" class='timepicker'>
  </div>



    <!-- calender starts here -->
    <div id='calendar'></div>

  <script
    src="http://code.jquery.com/jquery-3.3.1.js"
    integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60="
    crossorigin="anonymous"></script>
  <script
    src="http://code.jquery.com/ui/1.12.1/jquery-ui.js"
    integrity="sha256-T0Vest3yCU7pafRw9r+settMBX6JkKN06dqBnpQ8d30="
    crossorigin="anonymous"></script>
  <script src='https://fullcalendar.io/js/fullcalendar-3.1.0/lib/moment.min.js'></script>
  <script src='https://fullcalendar.io/js/fullcalendar-3.1.0/fullcalendar.min.js'></script>
  <script type="text/javascript" src="/scripts/date-time-picker.js"></script>


</body>
</html>
